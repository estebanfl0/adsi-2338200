Proceso ejercicio16
	//16. Calcular el área de un rectángulo de lados X e Y.
	
	//Definir variables 
	Definir ladX, ladY, area Como Real;
	
	//Proceso 
	Escribir "Escriba el alto del rectangulo";
	Leer ladX;
	Escribir "Escriba el ancho del rectangulo";
	Leer ladY;
	
	//Proceso 
	area <- ladX * ladY;
	
	Escribir "El area del rectangulo es ", area;
	

	 
FinProceso
