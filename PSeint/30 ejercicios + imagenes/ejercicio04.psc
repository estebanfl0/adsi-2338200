	//4. Hacer un algoritmo que lea dos números enteros A y B y muestre el resultado de realizar: (A + B) *2
	//+10
	
	//Definir variables
	Definir A, B Como Entero;
	Definir Resultado Como Real;
	
	//Proceso 
	Escribir "Digite el primer numero entero";
	Leer A;
	Escribir "Digite el segundo numero entero";
	Leer B;
	
	//Proceso 
	Resultado <- (A + B) * 2;
	
	//Salida
	Escribir "El resultado es ", Resultado;
	

	
FinProceso
