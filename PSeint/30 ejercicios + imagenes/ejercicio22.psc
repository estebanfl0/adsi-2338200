Proceso ejercicio22
	//22. Pedir dos números y mostrarlos ordenados de mayor a menor.
	
	//Definir variables 
	Definir Num1, Num2 Como Real;
	
	//Salida
	Escribir "Digite el primer numero ";
	//Entrada
	Leer Num1;
	//Salida
	Escribir "Digite el segundo numero";
	//Entrada 
	Leer Num2;
	
	//Proceso 
	Si (Num1>Num2) Entonces
		Escribir Num1;
		Escribir Num2;
	SiNo
		Si (Num2>Num1) Entonces
			Escribir Num2;
			Escribir Num1;
		FinSi
	FinSi

	
FinProceso
