Proceso ejercicio27 
	//27. Pedir un número y decir si es par o impar.
	
	//Definicion de variables 
	Definir Nmo Como Entero;
	
	//Salida 
	Escribir "Digite un numero entero";
	//Entrada 
	Leer Nmo;
	
	//Proceso 
	Si (Nmo % 2 = 0) Entonces
		Escribir "El numero es par";
	SiNo
		Escribir "El numero es impar";
	FinSi

	
FinProceso
